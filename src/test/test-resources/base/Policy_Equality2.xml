<wsp:Policy
	xmlns:sp="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy"
	xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy"
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	wsu:Id="Equality">
	<sp:SymmetricBinding>
		<wsp:Policy>
			<sp:EncryptionToken>
				<wsp:Policy>
					<sp:UsernameToken>
						<wsp:Policy>
							<sp:WssUsernameToken11 />
						</wsp:Policy>
					</sp:UsernameToken>
				</wsp:Policy>
			</sp:EncryptionToken>
			<sp:SignatureToken>
				<wsp:Policy>
					<sp:X509Token
						sp:IncludeToken="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy/IncludeToken/Once">
						<wsp:Policy>
							<sp:WssX509V1Token10 />
						</wsp:Policy>
					</sp:X509Token>
				</wsp:Policy>
			</sp:SignatureToken>
			<sp:AlgorithmSuite>
				<wsp:Policy>
					<wsp:ExactlyOne>
						<sp:Basic256 />
						<sp:TripleDes />
						<sp:Basic256Rsa15 />
					</wsp:ExactlyOne>
				</wsp:Policy>
			</sp:AlgorithmSuite>
			<sp:Layout>
				<wsp:Policy>
					<wsp:ExactlyOne>
						<sp:Lax />
						<sp:Strict />
					</wsp:ExactlyOne>
				</wsp:Policy>
			</sp:Layout>
			<sp:EncryptSignature />
			<sp:ProtectTokens />
			<sp:OnlySignEntireHeadersAndBody />
			<sp:IncludeTimestamp />
			<sp:EncryptBeforeSigning />
			<sp:EndorsingSupportingTokens>
				<wsp:Policy>
					<sp:X509Token
						sp:IncludeToken="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy/IncludeToken/Once">
						<wsp:Policy>
							<sp:WssX509V1Token10 />
						</wsp:Policy>
					</sp:X509Token>
					<sp:UsernameToken>
						<wsp:Policy>
							<wsp:ExactlyOne>
								<sp:WssUsernameToken10 />
								<sp:WssUsernameToken11 />
							</wsp:ExactlyOne>
						</wsp:Policy>
					</sp:UsernameToken>
					<sp:KerberosToken
						sp:IncludeToken="http://schemas.xmlsoap.org/ws/2005/07/securitypolicy/IncludeToken/Once">
						<wsp:Policy>
							<sp:WssGssKerberosV5ApReqToken11 />
						</wsp:Policy>
					</sp:KerberosToken>
					<sp:AlgorithmSuite>
						<wsp:Policy>
							<wsp:ExactlyOne>
								<sp:TripleDes />
								<sp:Basic256 />
							</wsp:ExactlyOne>
						</wsp:Policy>
					</sp:AlgorithmSuite>
					<sp:EncryptedParts>
						<sp:Body />
						<sp:Header Name="myEncryptedPartsA"
							Namespace="http://myNamespaceA" />
						<sp:Header Name="myEncryptedPartsB"
							Namespace="http://myNamespaceB" />
						<sp:Header Namespace="http://myNamespaceC" />
					</sp:EncryptedParts>
					<sp:SignedElements
						sp:XPathVersion="http://www.w3.org/TR/1999/REC-xpath-19991116">
						<sp:XPath>A/*</sp:XPath>
						<sp:XPath>B/*</sp:XPath>
						<sp:XPath>C/*</sp:XPath>
					</sp:SignedElements>
					<sp:SignedParts>
						<sp:Body />
						<sp:Header Name="mySignedPartsB"
							Namespace="http://myNamespaceB" />
						<sp:Header Name="mySignedPartsA"
							Namespace="http://myNamespaceA" />
						<sp:Header Namespace="http://myNamespaceC" />
					</sp:SignedParts>
					<sp:EncryptedElements
						sp:XPathVersion="http://www.w3.org/2002/06/xmldsig-filter2">
						<sp:XPath>X/*</sp:XPath>
						<sp:XPath>Y/*</sp:XPath>
						<sp:XPath>Z/*</sp:XPath>
					</sp:EncryptedElements>
				</wsp:Policy>
			</sp:EndorsingSupportingTokens>
		</wsp:Policy>
	</sp:SymmetricBinding>
</wsp:Policy>

