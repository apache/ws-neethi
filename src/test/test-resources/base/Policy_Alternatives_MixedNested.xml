<wsp:Policy wsu:Id="Policy_Alternatives_MixedNested"
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	xmlns:cal="http://policy.test.webservices.com/calendar"
	xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy">
	<wsp:ExactlyOne>
		<wsp:Policy>
			<cal:Hour>24</cal:Hour>
			<cal:Unknown>XXX</cal:Unknown>
		</wsp:Policy>
		<cal:Month>UNSUPPORTED_XXX</cal:Month>
	</wsp:ExactlyOne>
	<wsp:All>
		<wsp:ExactlyOne>
			<cal:YearDay>Numeric</cal:YearDay>
			<cal:Unknown>YYY</cal:Unknown>
		</wsp:ExactlyOne>
		<cal:MonthDay>UNSUPPORTED_YYY</cal:MonthDay>
	</wsp:All>
</wsp:Policy>

