  <description>Neethi API</description>
  <url>http://ws.apache.org/commons/neethi/</url>
  <issueManagement>
    <url>http://issues.apache.org/jira/browse/WSCOMMONS</url>
  </issueManagement>
  <ciManagement>
    <system>Continuum</system>
    <url>unknown</url>
    <notifiers>
      <notifier>
