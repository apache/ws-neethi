<wsp:Policy wsu:Id="Policy_Optional_Explosion"
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	xmlns:cal="http://policy.test.webservices.com/calendar"
	xmlns:util="http://policy.test.webservices.com/util"
	xmlns:form="http://policy.test.webservices.com/form"
	xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy">
	<wsp:All>
		<cal:Second wsp:Optional="true">Numeric</cal:Second>
		<cal:Minute wsp:Optional="true">Numeric</cal:Minute>
		<cal:Hour wsp:Optional="true">24</cal:Hour>
		<cal:Log wsp:Optional="true">ID</cal:Log>
		<cal:Month wsp:Optional="true">Alpha</cal:Month>
		<cal:MonthDay wsp:Optional="true">Numeric</cal:MonthDay>
		<cal:YearDay wsp:Optional="true">Numeric</cal:YearDay>
		<cal:Year wsp:Optional="true">Numeric</cal:Year>
		<cal:WeekDay wsp:Optional="true">Numeric</cal:WeekDay>
		<cal:Day wsp:Optional="true">Alpha</cal:Day>
		<util:Log wsp:Optional="true">Header</util:Log>
		<util:Log wsp:Optional="true">Time</util:Log>
		<util:Log wsp:Optional="true">ID</util:Log>
		<form:Shape wsp:Optional="true">Circle</form:Shape>
		<form:Size wsp:Optional="true">Small</form:Size>
		<form:Color wsp:Optional="true">Green</form:Color>
	</wsp:All>
</wsp:Policy>

