<wsp:Policy wsu:Id="Policy_Alternatives_PolicyNested"
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	xmlns:cal="http://policy.test.webservices.com/calendar"
	xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy">
	<wsp:ExactlyOne>
		<wsp:Policy>
			<cal:Hour>24</cal:Hour>
			<cal:Log>ID</cal:Log>
		</wsp:Policy>
		<wsp:All>
			<cal:Month>Alpha</cal:Month>
			<cal:MonthDay>Numeric</cal:MonthDay>
		</wsp:All>
	</wsp:ExactlyOne>
	<wsp:All>
		<wsp:Policy Name="second">
			<cal:YearDay>Numeric</cal:YearDay>
			<cal:Year>Numeric</cal:Year>
		</wsp:Policy>
	</wsp:All>
</wsp:Policy>

